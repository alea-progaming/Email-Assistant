{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "NEURespond",
  "description": "Email Assistant to improve your productivity and efficiency.",
  "short_name": "NEURespond",
  "version": "2.0.0.0",
  "version_name": "2.0.0",
  "icons": {
    "16": "common/assets/images/NEURespond_Logo-16.png",
    "48": "common/assets/images/NEURespond_Logo-48.png",
    "128": "common/assets/images/NEURespond_Logo-128.png"
  },
  "manifest_version": 3,
  "action": {
    "default_title": "Side Panel"
  },
  "side_panel": {
    "default_path": "common/index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-loader-ff936c33.js"
      ],
      "all_frames": false,
      "matches": [
        "*://mail.google.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "*://mail.google.com/*"
  ],
  "permissions": [
    "scripting",
    "activeTab",
    "sidePanel",
    "tabs",
    "webNavigation"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "*://*/*"
      ],
      "resources": [
        "chrome/content-script/index.ts",
        "common/assets/images/NEURespond_Logo-128.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "resources": [
        "assets/index-0e8d7234.js",
        "assets/index.ts-d633c340.js"
      ],
      "use_dynamic_url": true
    }
  ]
}
